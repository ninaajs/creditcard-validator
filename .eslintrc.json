{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style": ["error", "windows"],
		"indent": [2, "tab"],
		"no-tabs":0,
        "import/extensions": "off"	
    }
}
